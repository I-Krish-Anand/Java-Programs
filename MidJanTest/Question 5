public class Question5
{
    public static void main(String[] args)
    {
        String input=" Hello World Please";
        String ref="Help";
        for(char c:ref.toLowerCase().toCharArray())
        {
            int count=0;
            for(char a:input.toLowerCase().toCharArray())
            {
                if(c==a)
                    count++;
            }
            System.out.println(c+":"+count);
        }
    }
}
